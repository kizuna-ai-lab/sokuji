# Sokuji Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Better Auth Backend URL
# This is the URL for your Cloudflare Workers backend with Better Auth
# For local development: http://localhost:8787
# For production: https://sokuji-api.kizuna.ai
VITE_BACKEND_URL=http://localhost:8787

# PostHog Analytics (Optional)
# VITE_POSTHOG_KEY=your_posthog_key
# VITE_POSTHOG_HOST=https://app.posthog.com

# Feature Flags (Optional)
# VITE_ENABLE_AUTH=true
# VITE_ENABLE_QUOTA_TRACKING=true